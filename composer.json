{
    "name": "ArtMksh/laravelReCaptcha",
    "description": "ReCaptcha For Laravel.",
    "keywords": [
        "recaptcha",
        "captcha",
        "laravel",
        "laravel5",
        "laravel6"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "art-mksh",
            "email": "art-mksh@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|^9.5.10|^10.5"
    },
    "autoload": {
        "psr-4": {
            "ArtMksh\\ReCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtMksh\\laravelReCaptcha\\RecaptchaServiceProvider"
            ],
            "aliases": {
                "laravelReCaptcha": "art-mksh\\laravelReCaptcha\\Facades\\ReCaptcha"
            }
        }
    }
}
